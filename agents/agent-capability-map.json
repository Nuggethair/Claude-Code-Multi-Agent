  "selection_algorithms": {
    "complexity_based": {
      "description": "基于项目复杂度选择代理",
      "rules": {
        "simple": ["low", "medium"],
        "moderate": ["medium", "high"], 
        "complex": ["high", "very_high"],
        "enterprise": ["very_high"]
      }
    },
    "skill_matching": {
      "description": "基于技能匹配选择代理",
      "weight_factors": {
        "exact_match": 1.0,
        "related_skill": 0.8,
        "domain_overlap": 0.6,
        "general_capability": 0.4
      }
    },
    "workload_balancing": {
      "description": "基于工作负载平衡选择代理",
      "max_concurrent_tasks": 3,
      "prefer_available": true,
      "load_balancing": true
    }
  },
  "integration_patterns": {
    "sequential": "任务按顺序执行，一个完成后开始下一个",
    "parallel": "多个任务同时执行，适合独立任务",
    "pipeline": "任务形成流水线，输出作为下一个任务的输入",
    "collaborative": "多个代理协作完成单个复杂任务"
  }
} 